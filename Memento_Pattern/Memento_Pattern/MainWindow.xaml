<Window x:Class="Memento_Pattern.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="MainWindow" Height="350" Width="525">
    <Grid>
        <Canvas x:Name="canvas" HorizontalAlignment="Left" Height="274" VerticalAlignment="Top" Width="517" MouseDown="canvas_MouseDown" MouseMove="canvas_MouseMove" MouseUp="canvas_MouseUp" Background="White"/>
        <Button x:Name="saveBtn" Content="Save" HorizontalAlignment="Center" Margin="-85,289,0,0" VerticalAlignment="Top" Width="75" Click="saveBtn_Click"/>
        <Button x:Name="loadBtn" Content="Last state" HorizontalAlignment="Center" Margin="85,289,0,0" VerticalAlignment="Top" Width="75" Click="loadBtn_Click"/>

    </Grid>
</Window>
